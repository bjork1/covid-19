<div id="home">

  <div class="top-bar" id="blueBG">
    <div class="top-bar-left">
      <ul class="dropdown menu" id="blueText">
        <li class="menu-text">Covid-19 Outreach Hub</li>

      </ul>
    </div>
    <div class="top-bar-right">
      <ul class="menu">
        <li><input type="search" placeholder="Search"></li>
        <li><button type="button" class="button">Search</button></li>
      </ul>
    </div>
  </div>
</div>









<div class="grid-x">



  <div class="cell small-1">

  </div>



  <div class="cell small-5 ">
    <p class="h1">Hennepin County</p>



    <div class="grid-x">

      <div class="cell small-6">
        <div class="card">
          <div class="card-divider">
            Confirmed Cases
          </div>
          <div class="card-section">
            <h4 id='minneapolisData2'></h4>

          </div>
        </div>

      </div>
      <div class="cell small-6">
        <div class="card">
          <div class="card-divider">
            Death Count
          </div>
          <div class="card-section">
            <h4 id='minneapolisData3'></h4>

          </div>
        </div>

      </div>
    </div>


    <div class="callout primary">




      <form class="clearfix mb-4 float-center" action="POST">
        <h4>How can you help the community?</h4>
        <div class="form-group">
          <label class="formLabel" for="example-text">Title</label>
          <input type="text" id="example-text" class="form-control" aria-describedby="example-text"
            placeholder="Enter title">
        </div>
        <div class="form-group">
          <label class="formLabel" for="example-description">Description</label>
          <textarea class="form-control" id="example-description" aria-describedby="example-description"
            placeholder="Enter the description"></textarea>
        </div>

        <button id="submit" class="button primary float-center">Post</button>

      </form>


    </div>
  </div>

  <div class="cell small-1"></div>



  <div class="cell small-4">



    <table class="top">
      <thead>
        <tr>
          <th>Recent Posts</th>


        </tr>
      </thead>
      <tbody id="example-list">
        {{#each examples}}
        <tr>
          <td data-id="{{this.id}}"><a href="example/{{this.id}}">{{this.text}}</a>
            <p>{{this.description}}</p>





            <button class="button button-danger float-right delete">ï½˜</button>
          </td>

        </tr>

        {{/each}}
      </tbody>
    </table>



  </div>

  <div class="cell small-1">

  </div>


</div>






<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
